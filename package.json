  "scripts": {
    "setup": "npm ci && node setup.js",
    "start": "npm run dev",
    "dev": "node --watch -r dotenv/config src/index.js",
    "prod": "nodemon -r dotenv/config src/index.js",
    "deploy": "pm2 start src/index.js --name 'emmanuelb-state' --node-args='-r dotenv/config' || pm2 reload emmanuelb-state --update-env",
    "lint:style": "stylelint public/**/*.{css,scss}",
    "lint:script": "npx eslint src --ext .vue,.js,.jsx,.cjs,.mjs",
    "test": "vitest --run",
    "prepare": "husky"
  },
  "lint-staged": {
    "public/**/*.css": [
      "npx stylelint"
    ],
    "src/**/*.{js,jsx,cjs,mjs}": [
      "npx eslint"
    ],
    "public/**/*.{js,jsx,cjs,mjs}": [
      "npx eslint"
    ]
  },
  "dependencies": {
    "@fastify/basic-auth": "^6.2.0",
