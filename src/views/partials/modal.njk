<dialog class="log-details-modal" id="modal-{{ log.name }}" data-status="{{ log.status }}">
	<h3 class="log-details-name"><span class="log-status">{{ log.status }}</span> {{ log.name }}</h3>

	<div class="log-details-code">
		<pre><code class="language-accesslog">{{ log.content }}</code></pre>
	</div>

	<button type="button" class="button log-details-closer" autofocus>Fermer</button>
</dialog>
