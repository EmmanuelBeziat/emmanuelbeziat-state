<article class="log" data-status="{{ log.status }}" id="{{ log.name }}">
	<header class="log-header log-details-opener">
		<h2 class="log-title">{{ log.name }}</h2>
		<div class="log-status">{{ log.status }}</div>
		<time class="log-time" data-realtime timestamp="{{ log.date }}">{{ log.date | formatDateRelative }}</time>
	</header>

	{% include "partials/modal.njk" %}
</article>
